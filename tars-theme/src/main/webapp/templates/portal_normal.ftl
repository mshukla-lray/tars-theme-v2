<script>
    //start sub menu navigation dropdown//
    $('.header-sub__menu > li.dropdown').mouseenter(function() {
        $(this).addClass('active');
        $(this).find('.dropdown-menu').addClass('open');
        $('.search-box').css('display', 'none');
        $('.search').removeClass('active');
    });
    $('.header-sub__menu > li.dropdown').mouseleave(function() {
        $('.header-sub__menu > li.dropdown').removeClass('active');
        $('.header-sub__menu > li.dropdown > .dropdown-menu').removeClass('open');
        $('.header-sub__menu > li.dropdown > .dropdown-menu > .hs-menu > .hs-menu__tab > .ul-flex > li').removeClass('active');
        $('.header-sub__menu > li.dropdown > .dropdown-menu > .hs-menu > .hs-menu__tab > .ul-flex > li:first-child').addClass('active');
        $('.header-sub__menu > li.dropdown > .dropdown-menu > .hs-menu > .hs-menu__content > .ul-flex').addClass('none');
        $('.header-sub__menu > li.dropdown > .dropdown-menu > .hs-menu > .hs-menu__content > .ul-flex:first-child').removeClass('none');
        $('.search-box').css('display', 'none');
        $('.search').removeClass('active');
    });
    //end sub menu navigation dropdown//

    //start sub menu navigation dropdown submenu//
    $('.header-sub__menu > li.dropdown > .dropdown-menu > .hs-menu > .hs-menu__tab > .ul-flex > li').hover(function() {
        $('.header-sub__menu > li.dropdown > .dropdown-menu > .hs-menu > .hs-menu__tab > .ul-flex > li').removeClass('active');
        $(this).addClass('active');
        var tab_content = $(this).attr('data-attr');
        $('.header-sub__menu > li.dropdown > .dropdown-menu > .hs-menu > .hs-menu__content > .ul-flex').addClass('none');
        $('.header-sub__menu > li.dropdown > .dropdown-menu > .hs-menu > .hs-menu__content > .ul-flex.' + tab_content).removeClass('none');
        $('.search-box').css('display', 'none');
        $('.search').removeClass('active');
    });
    
    //end sub menu navigation dropdown submenu//
    //start notification dropdown//
    $('.notification').click(function() {
        $(this).toggleClass('active');
        if ($(this).hasClass('active')) {
            $(this).find('.notification-dropdown').addClass('open');
        } else {
            $(this).find('.notification-dropdown').removeClass('open');
        }
        $('.search-box').css('display', 'none');
        $('.search').removeClass('active');
    });
    //end notification dropdown//
    //Mobile Menus//
    var window_width = $(window).width();
    if (window_width <= 769) {
        $('.mobile-menulist__items > li.mobile-dropdown > a').click(function() {
            $(this).parent().siblings().removeClass('active');
            $(this).parent().toggleClass('active');
            $(this).parent().siblings().find('.mobile-dropdown').removeClass('active');
            $(this).parent().siblings().find('.mobile-dropdown__menu').removeClass('open');
            $(this).siblings('.mobile-dropdown__menu').toggleClass('open');
        });
        $('.mobile-menulist__items > li.mobile-dropdown > .mobile-dropdown__menu> ul > li.mobile-dropdown > a').click(function() {
            $(this).parent().siblings().removeClass('active');
            $(this).parent().toggleClass('active');
            $(this).parent().siblings().find('.mobile-dropdown__menu').removeClass('open');
            $(this).siblings('.mobile-dropdown__menu').toggleClass('open');
        });
    }
    //start footer nav link//
    if (window_width <= 990){
		$('.accTitle').click(function(){
			$(this).toggleClass('active');
			$(this).siblings('.list-foo-links').toggleClass('max-height');
		});    
    }
    $(window).scroll(function() {
        var position = window_width <= 769 ? 80 : 180;
        if ($(this).scrollTop() >= position) {
            $('header .scroll-header, .mobile').addClass('fixed');        
         } else {
            $('header .scroll-header, .mobile').removeClass('fixed');
        }
    });
    
    $('.search').click(function(){
    	$('.notification').removeClass('active');
    	$('.notification-dropdown').removeClass('open');
    });
    
	$("#searchKW_tars, #searchKW-MOB_tars").keypress(function(e){
		13==e.which&&($(this).val()!='')&&(location.href="${globalSearchPageURL}?tarsSearchkeyword="+encodeURI($(this).val()),
		e.preventDefault())
	});
</script>
